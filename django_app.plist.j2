<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.example.django_app</string>
    <key>ProgramArguments</key>
    <array>
        <string>{{ django_app_virtualenv }}/bin/python3</string>
        <string>{{ django_app_remote_path }}/manage.py</string>
        <string>runserver</string>
        <string>0.0.0.0:8000</string>
    </array>
    <key>WorkingDirectory</key>
    <string>{{ django_app_remote_path }}</string>
    <key>StandardOutPath</key>
    <string>{{ django_app_remote_path }}/logs/django_app.log</string>
    <key>StandardErrorPath</key>
    <string>{{ django_app_remote_path }}/logs/django_app_error.log</string>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
</dict>
</plist>